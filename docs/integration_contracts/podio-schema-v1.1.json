    },
    {
      "field_priority": 9,
      "supabase_table": "enriched_leads",
      "supabase_column": "validated_address",
      "podio_field_name": "Validated Mailing Address",
      "podio_field_id": 274896122,
      "field_type": "text",
      "business_rationale": "USPS-validated owner mailing address from Melissa Data. Deliverability confirmed. Used for direct mail campaigns when phone contact fails. Absentee owners (different from property address) scored higher.",
      "dialer_usage": {
