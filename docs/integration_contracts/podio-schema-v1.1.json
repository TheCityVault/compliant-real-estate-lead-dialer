  "enriched_fields": [
    {
      "field_priority": 1,
      "supabase_table": "enriched_leads",
      "supabase_column": "lead_score",
      "podio_field_name": "Lead Score",
      "podio_field_id": 274896114,
      "field_type": "number",
      "data_range": "0-100",
      "business_rationale": "AI-calculated quality score combining equity position (30pts), timeline urgency (10pts), source quality (40pts), contact quality (8pts), and property condition (12pts). Drives priority routing and agent workload distribution.",
      "dialer_usage": {
        "display_in_ui": true,
        "ui_section": "lead_intelligence_panel",
        "ui_priority": 1,
        "use_for_routing": true,
        "routing_threshold": 50,
        "display_format": "badge_with_number"
      },
      "sample_values": [
        65,
        78,
        92,
        45
      ],
      "null_handling": "required"
    },
    {
      "field_priority": 2,
      "supabase_table": "enriched_leads",
      "supabase_column": "lead_tier",
      "podio_field_name": "Lead Tier",
      "podio_field_id": 274896115,
      "field_type": "category",
      "allowed_values": [
        "HOT",
        "WARM",
        "COLD"
      ],
      "business_rationale": "Visual triage classification. HOT (80-100) = immediate contact required. WARM (50-79) = standard follow-up queue. COLD (<50) = long-term nurture. Simplifies agent prioritization without score calculations.",
      "dialer_usage": {
        "display_in_ui": true,
        "ui_section": "lead_intelligence_panel",
        "ui_priority": 2,
        "use_for_routing": true,
        "color_coding": {
          "HOT": "red",
          "WARM": "orange",
          "COLD": "blue"
        },
        "display_format": "colored_badge"
      },
      "sample_values": [
        "WARM",
        "HOT",
        "COLD"
      ],
      "null_handling": "required"
    },
    {
      "field_priority": 3,
      "supabase_table": "enriched_leads",
      "supabase_column": "estimated_property_value",
      "podio_field_name": "Estimated Property Value",
      "podio_field_id": 274896116,
      "field_type": "money",
      "data_range": "50000-5000000",
      "business_rationale": "Melissa Data AI valuation for deal qualification. Determines wholesale fee potential (typically 1-3% of property value). Filters out low-value properties below minimum deal threshold ($100k).",
      "dialer_usage": {
        "display_in_ui": true,
        "ui_section": "lead_intelligence_panel",
        "ui_priority": 3,
        "use_for_filtering": true,
        "filter_threshold_min": 100000,
        "display_format": "currency_usd"
      },
      "sample_values": [
        323000,
        450000,
        275000
      ],
      "null_handling": "nullable_with_warning"
    },
    {
      "field_priority": 4,
      "supabase_table": "enriched_leads",
      "supabase_column": "equity_percentage",
      "podio_field_name": "Equity %",
      "podio_field_id": 274896117,
      "field_type": "number",
      "data_range": "-50 to 100",
      "business_rationale": "Primary motivation indicator. Negative equity (underwater) = highest conversion probability. 0-20% equity = limited traditional sale options (wholesale competitive). >50% equity = lower motivation.",
      "dialer_usage": {
        "display_in_ui": true,
        "ui_section": "lead_intelligence_panel",
        "ui_priority": 4,
        "use_for_qualification": true,
        "qualification_tiers": {
          "critical_motivation": "< 0%",
          "high_motivation": "0-20%",
          "moderate_motivation": "20-50%"
        },
        "display_format": "percentage_with_color"
      },
      "sample_values": [
        15.6,
        -5.2,
        45.8
      ],
      "null_handling": "nullable_with_warning"
    },
    {
      "field_priority": 5,
      "supabase_table": "enriched_leads",
      "supabase_column": "estimated_equity",
      "podio_field_name": "Estimated Equity",
      "podio_field_id": 274896118,
      "field_type": "money",
      "data_range": "-500000 to 2000000",
      "business_rationale": "Calculated as (Property Value - Balance Due). Shows actual dollar amount available for negotiation. Negative = seller owes more than property worth (forced sale scenario).",
      "dialer_usage": {
        "display_in_ui": true,
        "ui_section": "financial_details",
        "ui_priority": 5,
        "display_format": "currency_usd_with_sign"
      },
      "sample_values": [
        70000,
        -25000,
        180000
      ],
      "null_handling": "nullable"
    },
    {
      "field_priority": 6,
      "supabase_table": "enriched_leads",
      "supabase_column": "year_built",
      "podio_field_name": "Year Built",
      "podio_field_id": 274896119,
      "field_type": "number",
      "data_range": "1800-2025",
      "business_rationale": "Property condition proxy. Pre-1970 homes score highest (12pts) due to rehab potential. Newer builds (2010+) score lower (2pts) as they have less value-add opportunity for wholesalers.",
      "dialer_usage": {
        "display_in_ui": true,
        "ui_section": "property_details",
        "ui_priority": 6,
        "use_for_filtering": true,
        "display_format": "year"
      },
      "sample_values": [
        1955,
        1993,
        2005
      ],
      "null_handling": "nullable"
    },
    {
      "field_priority": 7,
      "supabase_table": "enriched_leads",
      "supabase_column": "property_use_code",
      "podio_field_name": "Property Type",
      "podio_field_id": 274896120,
      "field_type": "category",
      "allowed_values": [
        "385_Single_Family",
        "369_Duplex",
        "388_Triplex",
        "360_Apartments",
        "Other"
      ],
      "business_rationale": "Melissa PropertyUseStandardized code. Validates residential properties only. Code 385 (Single Family) is ideal target. Multi-family (duplex/triplex) acceptable. Commercial properties excluded by pipeline.",
      "dialer_usage": {
        "display_in_ui": true,
        "ui_section": "property_details",
        "ui_priority": 7,
        "display_format": "text_label"
      },
      "sample_values": [
        "385_Single_Family",
        "369_Duplex"
      ],
      "null_handling": "nullable"
    },
    {
      "field_priority": 8,
      "supabase_table": "enriched_leads",
